{
  "name": "angular-safeguard",
  "version": "2.0.0",
  "description": "angular library for managing session, locker and cookie storage, has backup for in-memory if none are supported",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MikaAK/angular-safeguard.git"
  },
  "keywords": [
    "angular2",
    "angular",
    "ng2",
    "sessionStorage",
    "cookies",
    "safeguard",
    "angular-locker",
    "localStorage",
    "locker"
  ],
  "author": "Mika Kalathil <me@mikakalathil.ca>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MikaAK/angular-safeguard/issues"
  },
  "homepage": "https://github.com/MikaAK/angular-safeguard#readme",
  "scripts": {
    "build": "npm run build:esm && npm run build:umd",
    "build:esm": "npm run ngcompile",
    "build:esm:watch": "gulp build:esm:watch",
    "build:umd": "webpack --config webpack-umd.config.ts",
    "build:umd:watch": "npm run build:umd -- --watch",
    "build:watch": "concurrently --raw \"npm run build:umd:watch\" \"npm run build:esm:watch\"",
    "ci": "npm run lint && npm run test && npm run build && npm run docs",
    "clean:all": "npm run clean:tmp && rimraf node_modules",
    "clean:tmp": "rimraf coverage dist tmp docs",
    "codecov": "cat coverage/lcov.info | codecov",
    "docs": "compodoc -p tsconfig.json -d docs --disableCoverage",
    "explorer": "source-map-explorer ./dist/index.umd.js",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs",
    "lint": "npm run tslint 'src/**/*.ts'",
    "ngcompile": "node_modules/.bin/ngc -p tsconfig-aot.json",
    "postversion": "git push && git push --tags",
    "prebuild": "rimraf dist tmp",
    "prebuild:watch": "rimraf dist tmp",
    "prepublishOnly": "npm run ci",
    "preversion": "npm run ci",
    "test": "karma start",
    "test:watch": "karma start --auto-watch --no-single-run",
    "tslint": "tslint"
  },
  "peerDependencies": {
    "@angular/core": "^5.0.0"
  },
  "devDependencies": {
    "@angular/common": "^5.0.3",
    "@angular/compiler": "^5.0.3",
    "@angular/compiler-cli": "^5.0.3",
    "@angular/core": "^5.0.3",
    "@angular/platform-browser": "^5.0.3",
    "@angular/platform-browser-dynamic": "^5.0.3",
    "@compodoc/compodoc": "^1.0.0-beta.9",
    "@types/assert": "0.0.31",
    "@types/jasmine": "^2.5.47",
    "@types/karma": "^1.7.0",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.0",
    "@types/webpack": "^3.0.13",
    "@types/webpack-env": "^1.13.0",
    "awesome-typescript-loader": "^3.1.3",
    "codecov": "^3.0.0",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.1",
    "gh-pages": "^1.0.0",
    "gulp": "^3.9.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-espower-preprocessor": "^1.1.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-power-assert": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.3",
    "lodash": "^4.17.4",
    "mocha": "^4.0.1",
    "power-assert": "^1.4.4",
    "rimraf": "^2.6.1",
    "rxjs": "^5.3.1",
    "source-map-explorer": "^1.3.3",
    "testdouble": "^3.2.6",
    "to-string-loader": "^1.1.5",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.4.2",
    "webpack": "^3.6.0",
    "webpack-angular-externals": "^1.0.2",
    "webpack-rxjs-externals": "^1.0.0",
    "zone.js": "^0.8.10"
  }
}
